@{
    var User = ViewBag.currentUser;
   
}
    <div class="d-flex flex-wrap align-items-center">
        <div class="profile-img position-relative me-3 mb-3 mb-lg-0 profile-logo profile-logo1">
            <img src="@User.ImageURL" alt="User-Profile" class="theme-color-default-img img-fluid rounded-pill avatar-100">
            <img src="@User.ImageURL" alt="User-Profile" class="theme-color-purple-img img-fluid rounded-pill avatar-100">
            <img src="@User.ImageURL" alt="User-Profile" class="theme-color-blue-img img-fluid rounded-pill avatar-100">
            <img src="@User.ImageURL" alt="User-Profile" class="theme-color-green-img img-fluid rounded-pill avatar-100">
            <img src="@User.ImageURL" alt="User-Profile" class="theme-color-yellow-img img-fluid rounded-pill avatar-100">
            <img src="@User.ImageURL" alt="User-Profile" class="theme-color-pink-img img-fluid rounded-pill avatar-100">
        </div>
        <div class="d-flex flex-wrap align-items-center mb-3 mb-sm-0">
            <h4  class="me-2 h4">@User.FullName</h4>
            <input type="hidden" id="StudentId" value="@User.Id" />
            <span> - </span>
        </div>
    </div>
